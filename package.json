{
  "name": "relay-devtools-core",
  "private": true,
  "version": "0.0.1",
  "description": "Relay Modern Devtools",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/relayjs/relay-devtools"
  },
  "engines": {
    "node": ">=8.10"
  },
  "scripts": {
    "build": "yarn run build:chrome && yarn run build:standalone",
    "build:chrome": "NODE_ENV=production rm -rf lib/chrome && cross-env NODE_ENV=production webpack --config ./src/integrations/chrome/webpack.config.js",
    "build:standalone": "rm -rf lib/standalone && cross-env NODE_ENV=production webpack --config ./src/integrations/standalone/webpack.config.js",
    "start": "webpack-dev-server --config ./src/integrations/development/webpack.config.js -d --hot --inline --port 3000",
    "start:standalone": "webpack --config ./src/integrations/standalone/webpack.config.js --watch",
    "start:chrome": "rm -rf lib/ && cross-env NODE_ENV=development webpack --config ./src/integrations/chrome/webpack.config.js --watch --hide-modules",
    "dash:chrome": "rm -rf lib/ && cross-env NODE_ENV=development webpack-dashboard --config ./src/integrations/chrome/webpack.config.js --watch --hide-modules",
    "prettier": "find src -name '*.js' -or -name '*.css' -or -name '*.less' | xargs prettier --write",
    "prepublishOnly": "yarn install && yarn build",
    "test:chrome": "./src/integrations/chrome/test.js",
    "test:standalone": "node ./lib/standalone/launchApp.js",
    "bundle:chrome": "NODE_ENV=production yarn build:chrome && cd lib && zip -r relay-devtools-v`node -p \"require('./chrome/manifest').version\"`.chrome.zip chrome",
    "typecheck": "flow check"
  },
  "dependencies": {
    "font-awesome": "^4.7.0",
    "font-awesome-webpack": "^0.0.5-beta.2",
    "graphql-syntax-highlighter-react": "^0.3.5",
    "react": "^16.3.0",
    "react-animate-on-change": "^1.0.1",
    "react-dom": "^16.3.0",
    "react-highlight-words": "^0.14.0",
    "react-redux": "^5.0.7",
    "react-relay": "^1.6.2",
    "react-split-pane": "^0.1.77",
    "react-window": "^1.2.1",
    "redux": "^3.7.2",
    "update-notifier": "^2.4.0",
    "ws": "^3.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-syntax-object-rest-spread": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "9.0.0",
    "babel-loader": "^8.0.4",
    "babel-minify-webpack-plugin": "^0.3.1",
    "chrome-launcher": "^0.10.5",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.4",
    "duplicate-package-checker-webpack-plugin": "^2.1.0",
    "eslint": "^4.1.1",
    "eslint-config-fbjs": "^2.1.0",
    "eslint-plugin-babel": "5.1.0",
    "eslint-plugin-flowtype": "2.43.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "2.6.0",
    "eslint-plugin-react": "7.8.2",
    "eslint-plugin-relay": "^0.0.28",
    "file-loader": "^0.11.2",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^2.28.0",
    "lerna": "^2.8.0",
    "less": "^2.7.2",
    "less-loader": "^4.0.4",
    "node-notifier": "^5.3.0",
    "prettier": "1.13.6",
    "redux-batched-actions": "^0.4.1",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "redux-unhandled-action": "^1.3.0",
    "remote-redux-devtools": "^0.5.13",
    "style-loader": "^0.19.0",
    "url-loader": "^0.5.9",
    "webpack": "^3.10.0",
    "webpack-chrome-extension-reloader": "^0.8.3",
    "webpack-dev-server": "^2.4.5",
    "webpack-hot-middleware": "^2.10.0"
  },
  "prettier": {
    "requirePragma": true,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": false,
    "jsxBracketSameLine": true,
    "parser": "flow"
  },
  "workspaces": [
    "src/integrations/*"
  ]
}
